  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "alias": {
          "test": "./test",
          "underscore": "lodash",
          "questionModule": "./src/modules/question/",
          "quizModule": "./src/modules/quiz/",
          "util": "./src/util/"
        }
      }
    ]
  ]
}
